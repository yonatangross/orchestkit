{
  "name": "orchestkit",
  "version": "6.0.0",
  "description": "OrchestKit Claude Plugin - AI-assisted development toolkit",
  "scripts": {
    "build": "bash scripts/build-plugins.sh",
    "prebuild": "echo 'ðŸ”¨ Building plugins from source...'",
    "postbuild": "echo 'âœ… Build complete! Plugins assembled in plugins/ directory'",
    "generate:playground-data": "node scripts/generate-playground-data.js",
    "verify:playground-thumbnails": "node scripts/verify-playground-thumbnails.js",
    "test": "bash tests/run-all-tests.sh",
    "test:security": "bash tests/security/run-security-tests.sh",
    "test:skills": "bash tests/skills/structure/test-skill-md.sh",
    "test:agents": "bash tests/agents/test-agent-frontmatter.sh",
    "test:manifests": "bash tests/manifests/test-skill-uniqueness.sh && bash tests/manifests/test-manifest-dependencies.sh && bash tests/manifests/test-marketplace-ordering.sh && bash tests/manifests/test-plugin-orphan-skills.sh",
    "test:manifests:uniqueness": "bash tests/manifests/test-skill-uniqueness.sh",
    "test:manifests:deps": "bash tests/manifests/test-manifest-dependencies.sh",
    "test:manifests:ordering": "bash tests/manifests/test-marketplace-ordering.sh",
    "test:manifests:orphans": "bash tests/manifests/test-plugin-orphan-skills.sh",
    "test:indexes": "bash tests/indexes/test-index-generation.sh",
    "test:playgrounds": "bash tests/playgrounds/test-data-freshness.sh && bash tests/playgrounds/test-data-accuracy.sh",
    "test:playgrounds:freshness": "bash tests/playgrounds/test-data-freshness.sh",
    "test:playgrounds:accuracy": "bash tests/playgrounds/test-data-accuracy.sh",
    "typecheck": "cd src/hooks && npx tsc --noEmit",
    "verify:imports": "node scripts/verify-imports.js",
    "prepush": "./bin/git-hooks/pre-push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yonatangross/orchestkit.git"
  },
  "keywords": [
    "claude-code",
    "plugin",
    "ai",
    "development",
    "toolkit"
  ],
  "author": "Yonatan Gross",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  }
}
